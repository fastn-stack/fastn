-- component display-testimonial-card:
caption title:
string designation:
body body:
ftd.image-src src:

-- ftd.row:
background.solid: $inherited.colors.background.base
width.fixed.px: 540
padding.px: 20
spacing.fixed.px: 25

	-- ftd.image:
	src: $display-testimonial-card.src
	width.fixed.px: 120
	height.fixed.px: 120
	border-top-left-radius.percent: 50
	border-bottom-left-radius.percent: 50
	border-bottom-right-radius.percent: 50
	
